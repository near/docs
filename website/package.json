{
  "name": "near-docs",
  "version": "3.5.2",
  "private": true,
  "license": "UNLICENSED",
  "description": "Docusaurus website for NEAR Protocol documentation",
  "homepage": "https://docs.near.org",
  "engines": {
    "node": ">=18.0"
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf package-lock.json && rm -rf build",
    "pretty": "prettier . --check",
    "pretty:fix": "prettier . --write",
    "full-test": "./test-links.sh test",
    "start": "docusaurus start",
    "start:dev": "docusaurus serve --port 3000 --host localhost",
    "build": "./node_modules/.bin/docusaurus build && yarn run process-markdown",
    "build:dev": "./node_modules/.bin/docusaurus build --dev && yarn run process-markdown",
    "build:preview": "yarn run process-markdown && ./node_modules/.bin/docusaurus build --locale en && yarn run process-markdown",
    "process-markdown": "node ./scripts/copy-md-to-static.js",
    "swizzle": "docusaurus swizzle",
    "docusaurus": "docusaurus",
    "fetch-changelog": "node ./src/utils/getChangelogs.js && npm run docusaurus download-remote-near-changelog"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "@types/react": "^18.2.42",
    "buffer": "^6.0.3",
    "glob": "^11.0.3",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "prettier": "^3.4.2",
    "serve": "^14.2.4"
  },
  "dependencies": {
    "@docusaurus/core": "3.8.1",
    "@docusaurus/plugin-ideal-image": "3.8.1",
    "@docusaurus/plugin-sitemap": "3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@docusaurus/theme-mermaid": "3.8.1",
    "@near-js/tokens": "^2.2.4",
    "@near-wallet-selector/bitte-wallet": "^9.2.0",
    "@near-wallet-selector/core": "^9.2.0",
    "@near-wallet-selector/ethereum-wallets": "^9.2.0",
    "@near-wallet-selector/here-wallet": "^9.2.0",
    "@near-wallet-selector/hot-wallet": "^9.2.0",
    "@near-wallet-selector/ledger": "^9.2.0",
    "@near-wallet-selector/meteor-wallet": "^9.2.0",
    "@near-wallet-selector/modal-ui": "^9.2.0",
    "@near-wallet-selector/nightly": "^9.2.0",
    "@near-wallet-selector/react-hook": "^9.2.0",
    "@near-wallet-selector/sender": "^9.2.0",
    "@saucelabs/theme-github-codeblock": "^0.3.0",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "clsx": "^2.1.1",
    "crypto-browserify": "^3.12.1",
    "docusaurus-plugin-sass": "^0.2.6",
    "gleap": "^13.7.3",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.52.2",
    "near-api-js": "5.1.1",
    "posthog-js": "^1.257.2",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-bootstrap-typeahead": "^6.3.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.61.1",
    "react-markdown": "^10.1.0",
    "react-monaco-editor": "^0.59.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-toastify": "^11.0.5",
    "sass": "^1.89.2",
    "stream-browserify": "^3.0.0",
    "styled-components": "^6.1.19",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vm-browserify": "^1.1.2"
  },
  "resolutions": {
    "near-api-js": "5.1.1"
  }
}
